	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="author" content="3Fishmedia LTD">

	<!-- The styles -->
	<style type="text/css">
	  body {
		padding-bottom: 40px;
	  }
	  .sidebar-nav {
		padding: 9px 0;
	  }
	</style>

  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    "backend-application" %>
  <%= stylesheet_link_tag    "bootstrap-spacelab" %>
